{
	"name": "andrisborbas-com",
	"version": "4.0.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true npm run build",
		"clean": "git clean --interactive -dx --exclude .env.local --exclude .env.*.local --exclude node_modules/ --exclude .husky/",
		"dev": "next dev",
		"fix": "run-s fix:*",
		"fix:eslint": "eslint --fix",
		"fix:prettier": "prettier --write .",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore .",
		"lint:prettier": "prettier --check .",
		"lint:typescript": "tsc",
		"prepare": "husky install",
		"start": "next start",
		"typegen": "cf-content-types-generator -s CONTENTFUL_SPACE_ID -t CONTENTFUL_MANAGEMENT_TOKEN -o src/@types/generated"
	},
	"nano-staged": {
		"*": "prettier --write --ignore-unknown .",
		"*.{ts,tsx,js,jsx}": "eslint --fix"
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.0.11",
		"@chakra-ui/react": "^2.3.6",
		"@contentful/rich-text-react-renderer": "^15.12.1",
		"@contentful/rich-text-types": "^15.13.2",
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"@fontsource/lato": "^4.5.10",
		"@mdx-js/loader": "^2.1.5",
		"@mdx-js/react": "^2.1.5",
		"@next/bundle-analyzer": "^12.3.1",
		"@next/mdx": "^12.3.1",
		"autoprefixer": "^10.4.12",
		"cf-content-types-generator": "^2.2.2",
		"clsx": "1.2.1",
		"contentful": "^10.0.0-beta-v10.8",
		"contentful-management": "^10.18.1",
		"dotenv": "^16.0.3",
		"framer-motion": "^7.6.1",
		"next": "^12.3.1",
		"next-compose-plugins": "^2.2.1",
		"next-mdx-remote": "^4.1.0",
		"next-transpile-modules": "^9.1.0",
		"postcss": "^8.4.18",
		"postcss-import": "15.0.0",
		"postcss-preset-env": "7.8.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.55.0",
		"tailwindcss": "3.1.8"
	},
	"devDependencies": {
		"@tsconfig/next": "1.0.3",
		"@types/node": "^18.11.2",
		"@types/react": "^18.0.21",
		"@typescript-eslint/eslint-plugin": "5.40.1",
		"@typescript-eslint/parser": "5.40.1",
		"cross-env": "7.0.3",
		"eslint": "8.25.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-next": "12.3.1",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.1",
		"eslint-plugin-react": "7.31.10",
		"eslint-plugin-react-etc": "2.0.0",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-simple-import-sort": "8.0.0",
		"husky": "^8.0.1",
		"nano-staged": "0.8.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "0.1.13",
		"typescript": "^4.8.4"
	}
}
